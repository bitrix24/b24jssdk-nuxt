{
	"name": "@bitrix24/b24jssdk-nuxt",
	"version": "0.1.0",
	"author": "Bitrix",
	"license": "MIT",
	"description": "Bitrix24 REST API JS SDK for Nuxt",
	"keywords": [
		"bitrix24",
		"restapi",
		"nuxt"
	],
	"repository": {
		"type": "git",
		"url": "git+https://github.com/bitrix24/b24jssdk-nuxt.git"
	},
	"type": "module",
	"exports": {
		".": {
			"types": "./dist/types.d.ts",
			"import": "./dist/module.mjs",
			"require": "./dist/module.cjs"
		}
	},
	"main": "./dist/module.cjs",
	"types": "./dist/types.d.ts",
	"files": [
		"README.md",
		"LICENSE",
		"dist"
	],
	"publishConfig": {
		"access": "public"
	},
	"scripts": {
		"prepack": "nuxt-module-build build",
		"dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground",
		"dev": "nuxi dev playground",
		"dev:build": "nuxi build playground",
		"postinstall": "nuxt prepare",
		"_release": "npm run lint && npm run test && npm run prepack && changelogen --release && npm publish && git push --follow-tags",
		"release": "npm run test && npm run prepack",
		"lint": "eslint .",
		"test": "vitest run",
		"test:watch": "vitest watch",
		"test:types": "vue-tsc --noEmit && cd playground && vue-tsc --noEmit"
	},
	"dependencies": {
		"@nuxt/kit": "^3.13.2",
		"@bitrix24/b24jssdk": "file:../b24jssdk/packages/jssdk",
		
		"axios": "^1.7.7",
		"qs": "^6.13.0",
		"luxon": "^3.5.0",
		"protobufjs": "^7.4.0"
	},
	"peerDependencies": {},
	"devDependencies": {
		"@nuxt/devtools": "^1.6.0",
		"@nuxt/eslint-config": "^0.6.1",
		"@nuxt/module-builder": "^0.8.4",
		"@nuxt/schema": "^3.13.2",
		"@nuxt/test-utils": "^3.14.4",
		"@types/node": "latest",
		"eslint": "^9.13.0",
		"nuxt": "^3.13.2",
		"typescript": "latest",
		"vitest": "^2.1.4",
		"vue-tsc": "^2.1.8"
	}
}
