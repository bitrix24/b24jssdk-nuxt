{
  "name": "@bitrix24/b24jssdk-nuxt",
  "version": "0.1.2",
  "packageManager": "pnpm@10.7.1",
  "author": "Bitrix",
  "license": "MIT",
  "description": "Bitrix24 REST API JS SDK for Nuxt",
  "keywords": [
    "bitrix24",
    "restapi",
    "nuxt"
  ],
  "homepage": "https://bitrix24.github.io/b24jssdk/",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bitrix24/b24jssdk-nuxt.git"
  },
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types.d.mts",
      "import": "./dist/module.mjs"
    },
    "./runtime/*": "./dist/runtime/*"
  },
  "main": "./dist/module.mjs",
  "typesVersions": {
    "*": {
      ".": [
        "./dist/module.d.mts"
      ],
      "runtime/*": [
        "./dist/runtime/*"
      ]
    }
  },
  "files": [
    "README.md",
    "LICENSE",
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground",
    "prepack": "pnpm build",
    "build": "nuxt-module-build build",
    "dev": "DEV=true nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:generate": "nuxt generate playground",
    "dev:preview": "nuxt preview playground",
    "lint": "eslint .",
    "typecheck": "vue-tsc --noEmit",
    "test": "vitest"
  },
  "dependencies": {
    "@nuxt/kit": "^3.16.2",
    "@nuxt/schema": "^3.16.2",
    "@bitrix24/b24jssdk": "^0.2.0",
    "axios": "^1.8.4",
    "qs": "^6.14.0",
    "luxon": "^3.6.1"
  },
  "devDependencies": {
    "@types/luxon": "^3.6.2",
    "@nuxt/devtools": "^2.4.0",
    "@nuxt/eslint-config": "^1.3.0",
    "@nuxt/module-builder": "^1.0.1",
    "@nuxt/test-utils": "^3.17.2",
    "@types/node": "^22.14.1",
    "eslint": "^9.25.1",
    "nuxt": "^3.16.2",
    "typescript": "^5.8.3",
    "vitest": "^3.1.2",
    "vue-tsc": "^2.2.10"
  }
}
